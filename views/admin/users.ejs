<div>
    <div class="mt-5 mb-3 text-center" style="margin-right: 20px;">
        <h4 style="font-family: serif;font-weight: bold;">All User List</h4>
    </div>
    <table class="table table-dark">
        <thead>
          <tr>
            <th scope="col">s.nu</th>
            <th scope="col">User Name</th>
            <th scope="col">Gender</th>
            <th scope="col">Email</th>
            <th scope="col">Contact</th>
          </tr>
        </thead>
        <tbody>
          <% for(let i = 0; i < users.length; i++){ %>
            <tr>
                <td><%= i + 1; %></td>
                <td><%= users[i].fname +' '+ users[i].lname; %></td>
                <td><%= users[i].gender; %></td>
                <td><%= users[i].email; %></td>
                <td><%= users[i].contact; %></td>
            </tr>
            <% } %>
        </tbody>
      </table>
</div>